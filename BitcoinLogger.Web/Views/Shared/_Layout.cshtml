<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title | BitcoinLogger</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link href="~/Content/custom/dataTables.css" rel="stylesheet" />
</head>
<body>


    @Html.Partial("_navbar")

    @Html.Partial("_heroImage")

    <div class="container body-content">
        @RenderBody()
 
        @Html.Partial("_footer")

    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)



    <script src="~/Scripts/custom/dataTables.js"></script>
    <script src="~/Scripts/custom/Main.js"></script>


</body>
</html>
